(
    (defn :int8 add8 ((a :int8) (b :int8))
        (+ a b)
    )
    (defn :int16 add16 ((a :int16) (b :int16))
        (+ a b)
    )
    (defn :int32 add32 ((a :int32) (b :int32))
        (+ a b)
    )
    (defn :int64 add64 ((a :int64) (b :int64))
        (+ a b)
    )
    (defn :float32 addf32 ((a :float32) (b :float32))
        (+ a b)
    )
    (defn :float64 addf64 ((a :float64) (b :float64))
        (+ a b)
    )
    
    (= x8 :int8 10)
    (= y8 :int8 20)
    (= x16 :int16 10)
    (= y16 :int16 20)
    (= x32 :int32 10)
    (= y32 :int32 20)
    (= x64 :int64 10)
    (= y64 :int64 20)

    (= res8 :int8 (add8 x8 y8))
    (= res16 :int16 (add16 x16 y16))
    (= res32 :int32 (add32 x32 y32))
    (= res64 :int64 (add32 x64 y64))

)